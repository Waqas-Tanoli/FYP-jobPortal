{
  "kind": "collectionType",
  "collectionName": "jobs",
  "info": {
    "singularName": "job",
    "pluralName": "jobs",
    "displayName": "Job",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "unique": false
    },
    "remoteOk": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean"
    },
    "datePosted": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "date"
    },
    "jobType": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Full-Time",
        "Part-Time",
        "Internship",
        "Contract"
      ]
    },
    "jobDescription": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "blocks"
    },
    "education": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Matric",
        "F.Sc",
        "Bachelors",
        "Masters",
        "Ph.D",
        "M.phill"
      ]
    },
    "salary": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "biginteger"
    },
    "experienceLevel": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Tech-lead",
        "Senior",
        "Mediocre",
        "Junior",
        "Fresher"
      ]
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "required": true
    },
    "jobs": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::company.company",
      "inversedBy": "company"
    },
    "skills_tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag",
      "mappedBy": "job"
    },
    "Industry": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Business",
        "Banking",
        "Education",
        "Telecommunication",
        "others"
      ]
    },
    "location": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "Company": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "apply_job": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::apply-job.apply-job",
      "inversedBy": "jobs"
    }
  }
}
